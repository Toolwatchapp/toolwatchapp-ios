{
  "name": "toolwatch-mobile",
  "author": "Mathieu Nayrolles",
  "homepage": "https://toolwatch.io/",
  "private": true,
  "scripts": {
    "build": "ionic-app-scripts build && npm run assets",
    "watch": "ionic-app-scripts watch",
    "ionic:serve": "ionic-app-scripts serve",
    "serve:before": "assets",
    "emulate:before": "build",
    "deploy:before": "build",
    "build:before": "build",
    "run:before": "build",
    "assets": "npm run i18n && npm run twwatches",
    "i18n": "rm -Rf src/assets/i18n && mkdir src/assets/i18n && cp node_modules/tw-i18n/lang/*.json src/assets/i18n",
    "twwatches": "rm -Rf src/assets/watches && mkdir src/assets/watches && cp -R node_modules/tw-watches/* src/assets/watches"
  },
  "dependencies": {
    "@angular/common": "2.1.1",
    "@angular/compiler": "2.1.1",
    "@angular/compiler-cli": "2.1.1",
    "@angular/core": "2.1.1",
    "@angular/forms": "2.1.1",
    "@angular/http": "2.1.1",
    "@angular/platform-browser": "2.1.1",
    "@angular/platform-browser-dynamic": "2.1.1",
    "@angular/platform-server": "2.1.1",
    "@ionic/storage": "1.1.6",
    "gsap": "^1.19.0",
    "ionic-angular": "2.0.0-rc.2",
    "ionic-native": "2.2.3",
    "ionicons": "3.0.0",
    "moment": "2.15.1",
    "rxjs": "5.0.0-beta.12",
    "tw-core": "git+https://github.com/Toolwatchapp/tw-core.git",
    "tw-i18n": "git+https://github.com/Toolwatchapp/tw-i18n.git",
    "tw-watches": "git+https://github.com/Toolwatchapp/tw-watches.git",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^0.0.43",
    "@types/jasmine": "^2.5.35",
    "@types/node": "^6.0.45",
    "cz-conventional-changelog": "^1.2.0",
    "typescript": "^2.0.6",
    "bulma": "0.2.3",
    "core-js": "^2.4.0",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "highlight.js": "^9.1.0",
    "html-loader": "^0.4.3",
    "intern": "^3.3.2",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-typescript": "^2.1.2",
    "karma-webpack": "^1.8.0",
    "marked": "^0.3.6",
    "node-sass": "^3.6.0",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.3",
    "remap-istanbul": "^0.7.0",
    "rxjs": "5.0.0-beta.12",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.0.0",
    "typings": "^0.6.8",
    "webpack": "2.1.0-beta.25",
    "zone.js": "0.6.25"
  },
  "description": "tw-mobile-2rc0: An Ionic project",
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "version": "0.9.1",
  "cordovaPlatforms": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
